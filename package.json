    "model",
    "viewer",
    "engine"
  ],
  "author": "Rothcold",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rothcold/euclid/issues"
  },
  "homepage": "https://github.com/rothcold/euclid#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-typescript": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "concurrently": "^6.5.1",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "husky": "^7.0.4",
    "prettier": "^2.5.1",
    "rollup": "^2.62.0",
    "serve": "^13.0.2",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "@types/three": "^0.135.0",
    "three": "^0.136.0"
  }
