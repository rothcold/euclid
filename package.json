{
  "name": "euclid",
  "version": "1.0.0",
  "description": "Easy to use 3D Model Viewer with toolbar",
  "main": "dist/euclid.cjs.js",
  "module": "dist/euclid.esm.js",
  "browser": "dist/euclid.umd.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "concurrently \"rollup -c -w\" \"serve .\"",
    "postinstall": "husky install",
    "lint": "eslint -c .eslintrc.cjs ./src/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rothcold/euclid.git"
  },
  "keywords": [
